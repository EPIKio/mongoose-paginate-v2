{
  "name": "mongoose-paginate-v2",
  "version": "5.13.0",
  "description": "A custom pagination library for Mongoose with customizable labels.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "pretest": "npm run build",
    "test": "./node_modules/.bin/mocha -r ts-node/register tests/**/*.ts -R spec --ui bdd --timeout 9999999",
    "build": "tsc -p tsconfig.build.json",
    "lint": "./node_modules/.bin/eslint \"tests\" \"src\" \".eslintrc.js\" --fix",
    "prettier": "./node_modules/.bin/prettier --write \"{src,tests}/**/*.ts\" \"*.md\"",
    "prepublish": "npm run test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "./node_modules/.bin/lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "./node_modules/.bin/eslint --fix",
      "./node_modules/.bin/prettier --write"
    ],
    "*.md": [
      "./node_modules/.bin/prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aravindnc/mongoose-paginate-v2.git"
  },
  "keywords": [
    "mongoose",
    "pagination",
    "plugin",
    "mongodb",
    "paginate",
    "paging",
    "next",
    "prev",
    "nextpage",
    "prevpage",
    "total",
    "paginator",
    "plugin"
  ],
  "author": "Aravind NC <aravind_n_c@yahoo.co.in> (https://aravindnc.com)",
  "contributors": [
    "Fernando López Guevara (fernando.lguevara@gmail.com)",
    "Daniel Seijo Sánchez (daniseijo.dev@gmail.com)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aravindnc/mongoose-paginate-v2/issues"
  },
  "homepage": "https://github.com/aravindnc/mongoose-paginate-v2#readme",
  "dependencies": {},
  "peerDependencies": {
    "mongoose": "^5.13.x"
  },
  "devDependencies": {
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "babel-eslint": "^10.1.0",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "mocha": "^9.1.1",
    "mongodb": "^3.6.11",
    "mongoose": "^5.13.2",
    "prettier": "^2.3.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2"
  },
  "engines": {
    "node": ">=4.0.0"
  }
}
